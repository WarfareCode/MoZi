<!--类 'Command.CommandWPFResourceDictionary' 有 IComponentConnector.Connect 方法，但无法解析。-->
<ResourceDictionary
	x:Class="Command.CommandWPFResourceDictionary" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:command="clr-namespace:Command">
	<Style
		TargetType="{x:Type Button}"
		x:Key="{x:Type Button}">
		<Setter
			Property="Margin"
			Value="5" />
		<Setter
			Property="Padding"
			Value="5" />
	</Style>
</ResourceDictionary>
